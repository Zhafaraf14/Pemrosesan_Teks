        "# --- INISIALISASI MODEL ---\n",
        "model = AutoModelForSequenceClassification.from_pretrained(\n",
        "    PRETRAINED_MODEL,\n",
        "    num_labels=3 # Negatif, Netral, Positif\n",
        ")\n",
        "model.to(device)\n",
        "\n",
        "# Optimizer\n",
        "optimizer = AdamW(model.parameters(), lr=2e-5) # Learning rate kecil agar tidak merusak pre-trained weights\n",
        "epochs = 4 # Jumlah putaran training (biasanya 3-5 cukup)\n",
        "\n",
        "# --- FUNGSI TRAINING ---\n",
        "def train_epoch(model, data_loader, optimizer, device):\n",
        "    model.train()\n",
        "    total_loss = 0\n",
        "    correct_predictions = 0\n",
        "    n_examples = 0\n",
        "\n",
        "    for batch in data_loader:\n",
        "        input_ids = batch['input_ids'].to(device)\n",
        "        attention_mask = batch['attention_mask'].to(device)\n",
        "        labels = batch['labels'].to(device)\n",
        "\n",
        "        outputs = model(\n",
        "            input_ids=input_ids,\n",
        "            attention_mask=attention_mask,\n",
        "            labels=labels\n",
        "        )\n",
        "\n",
        "        loss = outputs.loss\n",
        "        logits = outputs.logits\n",
        "\n",
        "        _, preds = torch.max(logits, dim=1)\n",
        "        correct_predictions += torch.sum(preds == labels)\n",
        "        n_examples += labels.shape[0]\n",
        "\n",
        "        total_loss += loss.item()\n",
        "\n",
        "        loss.backward()\n",
        "        optimizer.step()\n",
        "        optimizer.zero_grad()\n",
        "\n",
        "    return correct_predictions.double() / n_examples, total_loss / len(data_loader)\n",
        "\n",
        "print(\"Mulai Training IndoBERT...\")\n",
        "for epoch in range(epochs):\n",
        "    print(f\"Epoch {epoch + 1}/{epochs}\")\n",
        "    train_acc, train_loss = train_epoch(model, train_loader, optimizer, device)\n",
        "    print(f\"  Train loss: {train_loss:.4f} | Train accuracy: {train_acc*100:.2f}%\")"
      ],
