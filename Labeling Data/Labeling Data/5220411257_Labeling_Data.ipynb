        "# 3. Load Data\n",
        "df = pd.read_csv('scraping_vios_1000.csv') # Sesuaikan nama file\n",
        "# Ambil kolom teks (sesuaikan namanya)\n",
        "kolom_teks = 'teks_komentar'"
      ],
      "metadata": {
        "id": "jodh3g9kkgxQ"
      },
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 4. Proses Labeling\n",
        "hasil_label = []\n",
        "hasil_score = []\n",
        "\n",
        "print(\"Mulai Labeling...\")\n",
        "for text in tqdm(df[kolom_teks]):\n",
        "    try:\n",
        "        # Potong teks jika terlalu panjang (maks 512 karakter)\n",
        "        text = str(text)[:512]\n",
        "        result = nlp(text)[0]\n",
        "\n",
        "        # Mapping hasil (model ini outputnya: positive, negative, neutral)\n",
        "        label = result['label']\n",
        "        score = result['score']\n",
        "\n",
        "        hasil_label.append(label)\n",
        "        hasil_score.append(score)\n",
        "    except Exception as e:\n",
        "        hasil_label.append(\"error\")\n",
        "        hasil_score.append(0)\n",
        "\n",
        "df['label_otomatis'] = hasil_label\n",
        "df['confidence'] = hasil_score\n",
        "\n",
        "# Cek Hasil\n",
        "print(df[[kolom_teks, 'label_otomatis']].head())\n"
      ],
